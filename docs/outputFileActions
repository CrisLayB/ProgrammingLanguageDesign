soy_un_id -> Token Action: ID
  -> Token Action: WHITESPACE
12 -> Token Action: NUMBER
  -> Token Action: WHITESPACE
if -> Token Action: IF

 -> Token Action: WHITESPACE
; -> Token Action: SEMICOLON
  -> Token Action: WHITESPACE
:= -> Token Action: ASSIGNOP
  -> Token Action: WHITESPACE
< -> Token Action: LT
  -> Token Action: WHITESPACE
= -> Token Action: EQ
  -> Token Action: WHITESPACE
+ -> Token Action: PLUS
  -> Token Action: WHITESPACE
- -> Token Action: MINUS
  -> Token Action: WHITESPACE
* -> Token Action: TIMES
  -> Token Action: WHITESPACE
/ -> Token Action: DIV
  -> Token Action: WHITESPACE
( -> Token Action: LPAREN
  -> Token Action: WHITESPACE
) -> Token Action: RPAREN
