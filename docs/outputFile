soy_un_id -> Token: id
  -> Token: ws
12 -> Token: number
  -> Token: ws
$ -> Token: ERROR LEXICO
  -> Token: ws
if -> Token: if

 -> Token: ws
; -> Token: ;
  -> Token: ws
:= -> Token: :=
  -> Token: ws
< -> Token: <
  -> Token: ws
= -> Token: =
  -> Token: ws
+ -> Token: +
  -> Token: ws
- -> Token: -
  -> Token: ws
* -> Token: *
  -> Token: ws
/ -> Token: /
  -> Token: ws
( -> Token: (
  -> Token: ws
) -> Token: )
