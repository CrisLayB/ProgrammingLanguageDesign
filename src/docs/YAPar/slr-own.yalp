/* Configuración del parser para Gramática Propia */
/* Sigue la gramática SLR: */
/* E → E + T | T */
/* T → T ∗ F | F */
/* F → ( E ) | id */

%token ID
%token PLUS
%token TIMES
%token LPAREN RPAREN

/* FINALIZA sección de TOKENS */

%%

/* INICIA sección de PRODUCCIONES */

e:
    e PLUS t
  | t
;
t:
    t TIMES f
  | f
;
f:
    LPAREN e RPAREN
  | ID
;